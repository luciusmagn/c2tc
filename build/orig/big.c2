func char lll_flags2char(int32 flags) {
    char f = ' ';
    switch (flags) {
    case NORTH:
        f = 'N';
        break;
    case EAST:
        f = 'E';
        break;
    case SOUTH:
        f = 'S';
        break;
    case WEST:
        f = 'W';
        break;
    case HEAD:
        f = 'H';
        break;
    case TAIL:
        f = 'T';
        break;
    }
