func int32 ddd_board_try_option(const DddOption* pp, int32 x, int32 y) {
    for (uint32 i=0; i<4; i++) {
        DddCoor c = pp.parts[i];
        if (i != 0 && c.x == 0 && c.y == 0) return 0;    // ignore unused parts
        int32 xx = x + c.x;
