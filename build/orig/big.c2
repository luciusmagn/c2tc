func VvvCoor vvv_coor_rotate(VvvCoor input, uint32 rotation) @(unused) {
    int8 x = input.x;
    int8 y = input.y;
    int8 t;
    switch (rotation) {
    case 0: // nothing to do
        break;
    case 1: // 90 degrees right
        t = x;
        x = y;
        y = -t;
        break;
    case 2: // 180 degrees right
        x = -x;
        y = -y;
        break;
    case 3: // 270 degrees right
        t = x;
        x = -y;
        y = t;
        break;
    }
    uint8 flags = input.flags;  // keep HEAD/TAIL, rotate NESW
    if (rotation > 0 && flags >= NORTH && flags <= WEST) {
        flags += rotation;
        if (flags > 4) flags -= 4;
    }

    VvvCoor result = { x, y, flags }
    return result;
}

