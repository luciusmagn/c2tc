    cp += io.sprintf(cp, "Board: Head=%d  Tail=%d\n", sss_board.haveHead, sss_board.haveTail);
    for (int32 y=4; y>=0; y--) {
        cp += io.sprintf(cp, "---------------------\n");
        for (int32 x=0; x<5; x++) {
            *cp++ = '|';
            SssField field = sss_board.fields[x + y * 5];
            switch (field.status) {
            case FIELD_FREE:
