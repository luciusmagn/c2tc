            IiiOption* o1 = &iii_options[o][p];
            for (uint32 i=0; i<4; i++) {  // side1
                IiiCoor c = pp.side1[i];
                if (i != 0 && c.x == 0 && c.y == 0) break;
                o1.parts[i] = iii_coor_rotate(c, o);
            }
            IiiOption* o2 = &iii_options[o+4][p];
            for (uint32 i=0; i<4; i++) {  // side2
