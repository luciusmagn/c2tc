func void lll_board_init() {
    LllField f;
    f.status = FIELD_FREE;
    f.piece_id = 0;
    f.flags = 0;
    f.expected = 0;
    for (int32 y=0; y<5; y++) {
        for (int32 x=0; x<5; x++) {
            lll_board_set(x, y, f);
        }
    }
    f.status = FIELD_BLOCKED;
    lll_board_set(0, 4, f);
    lll_board.haveHead = 0;
    lll_board.haveTail = 0;
